{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/frontend/Modules/access-control-I/login.html" },
    { "source": "/login", "destination": "/frontend/Modules/access-control-I/login.html" },
    { "source": "/modules/(.*)", "destination": "/frontend/Modules/$1" },
    { "source": "/public/(.*)", "destination": "/frontend/Public/$1" }
  ]
}